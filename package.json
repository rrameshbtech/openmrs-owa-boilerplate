{
  "name": "openmrs-owa-boilerplate",
  "version": "1.0.0",
  "description": "Sample Boilerplate for building OpenMRS owa apps based on PWA",
  "main": "index.jsx",
  "private": true,
  "keywords": [
    "React",
    "OpenMRS",
    "OWA"
  ],
  "author": "",
  "license": "MIT",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "openmrs-stop": "cd openMRS && docker-compose stop && docker-compose rm -fv",
    "openmrs-up": "cd openMRS && docker-compose up -d",
    "openmrs-start": "npm run openmrs-stop && npm run openmrs-up",
    "build": "webpack --config webpack.config.js --mode production",
    "scan": "npx hawkeye scan",
    "dev": "webpack --mode development --watch"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@hawkeyesec/scanner-cli": "^1.6.2",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.4",
    "core-js": "^3.1.4",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-formatter-friendly": "^7.0.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.18.2",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "engines": {
    "node": "12.7.0",
    "npm": "6.10.2"
  }
}
